{
  "name": "@habx/react-query-state",
  "version": "2.1.0",
  "description": "Hook to manage url query params as default state & persist local state",
  "main": "./dist/index.cjs.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/typings/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "bili",
    "build:watch": "npm run clean && tsc --watch",
    "generate:snapshots": "node ./scripts/generateSnapshot.js",
    "lint": "eslint \"src/**\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "type:coverage": "tscov"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habx/react-query-state.git"
  },
  "author": "HABX Front End Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/habx/react-query-state/issues"
  },
  "homepage": "https://github.com/habx/react-query-state#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "qs": "^6.10.1",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@habx/eslint-config-client": "^5.5.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.177",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.35",
    "bili": "^5.0.5",
    "husky": "^4.3.8",
    "jest": "^27.3.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^12.0.3",
    "lodash": "^4.17.21",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-typescript2": "^0.31.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "publishConfig":{
    "registry":  "https://npm.pkg.github.com/"
  }
}
